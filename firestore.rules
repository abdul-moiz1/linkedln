rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /templates/{templateId} {
      allow read: if true;
      allow write: if true; 
    }
    match /carouselTemplates/{templateId} {
      allow read: if true;
      allow write: if true;
    }
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
